<form method="post" action="{{ path('admin_etape_delete', {'id': etape.id}) }}" onsubmit="return confirm('Voulez-vous vraiment supprimer cette étape?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ etape.id) }}">
    <button class="btn">Supprimer cette étape</button>
</form>
